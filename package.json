{
  "name": "crete",
  "version": "1.0.0",
  "private": true,
  "main": "lib/index.js",
  "engines": {
    "node": "10.19.0"
  },
  "scripts": {
    "dev": "nodemon ./src/index.js --exec babel-node --delay 2",
    "start": "NODE_ENV=production node ./lib/index.js",
    "restart": "yarn && yarn dev",
    "build": "NODE_ENV=production rm -rf ./lib && mkdir -p lib && babel ./src -d ./lib",
    "test": "jest",
    "test:watch": "jest --maxWorkers=2 --watch",
    "lint": "eslint ./src --ext .js",
    "postinstall": "yarn build",
    "codecov": "cat ./coverage/lcov.info | codecov",
    "schema:publish": "apollo schema:publish",
    "loadtest": "wrk -s loadtests/userFeed.lua https://apollos-ns-production-herokuapp-com.global.ssl.fastly.net",
    "translate": "node scripts/translate.js"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test|tests).{js,jsx,mjs}"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "@apollosproject/config": "1.4.0",
    "@apollosproject/data-connector-algolia-search": "1.4.0",
    "@apollosproject/data-connector-analytics": "1.4.0",
    "@apollosproject/data-connector-bible": "1.4.0",
    "@apollosproject/data-connector-church-online": "1.4.0",
    "@apollosproject/data-connector-cloudinary": "1.4.0",
    "@apollosproject/data-connector-onesignal": "1.4.0",
    "@apollosproject/data-connector-passes": "1.4.0",
    "@apollosproject/data-connector-redis-cache": "1.4.0",
    "@apollosproject/data-connector-rock": "1.4.0",
    "@apollosproject/data-connector-twilio": "1.4.0",
    "@apollosproject/data-schema": "1.4.0",
    "@apollosproject/rock-apollo-data-source": "1.4.0",
    "@apollosproject/server-core": "1.4.0",
    "@bugsnag/js": "^6.2.0",
    "@bugsnag/plugin-express": "^6.2.0",
    "apollo-server": "^2.11.0",
    "apollo-server-express": "2.11.0",
    "color": "3.0.0",
    "dotenv": "6.0.0",
    "express": "^4.17.0",
    "graphql": "14.6.0",
    "prettier": "1.12.1",
    "randomcolor": "0.5.3"
  },
  "devDependencies": {
    "@apollosproject/apollo-server-env-mock": "1.4.0-beta.1",
    "@apollosproject/babel-preset-apollos": "1.4.0-beta.1",
    "@babel/cli": "^7.4.5",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.7.4",
    "@entria/graphql-findbreakingchanges": "^1.0.1",
    "apollo": "^1.9.2",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "codecov": "3.6.5",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-babel-module": "^5.0.0-beta.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.15.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.16.0",
    "get-graphql-schema": "^2.1.2",
    "graphql-schema-linter": "^0.2.0",
    "jest": "^24.9.0",
    "jest-fetch-mock": "^2.1.2",
    "linkemon": "^0.1.1",
    "nodemon": "^2.0.2",
    "standard-version": "^7.0.1",
    "@apollosproject/eslint-config": "1.4.0-beta.1",
    "eslint-config-airbnb-base": "^14.0.0"
  }
}